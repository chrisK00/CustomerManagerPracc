					"listen": "test",
					"script": {
						"exec": [
							"const customer = pm.response.json();\r",
							"\r",
							"pm.test(\"Has properties\", function () {\r",
							"    pm.expect(customer).to.have.property('username');\r",
							"    pm.expect(customer).to.have.property('token');\r",
							"});\r",
							"\r",
							"if (pm.test(\"Has properties\")) {\r",
							"    pm.globals.set('token', customer.token);\r",
							"}"
						],
						"type": "text/javascript"
					}
				}
